{
    "name": "webbuilders-group/silverstripe-responsivegridfield",
    "description": "Responsive addon for frontendgridfield that makes a SilverStripe GridField work better on various screensizes.",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "gridfield", "responsive"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ed Chipman",
            "homepage": "http://webbuildersgroup.com",
            "role": "Developer"
        }
    ],
    "require":
    {
        "silverstripe/framework": "~4.3",
        "webbuilders-group/silverstripe-frontendgridfield": "~2.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "expose": [
            "css"
        ]
    },
    "support": {
        "issues": "https://github.com/webbuilders-group/silverstripe-responsivegridfield/issues"
    },
    "autoload": {
        "psr-4": {
            "WebbuildersGroup\\ResponsiveGridField\\Forms\\GridField\\": "src/"
        }
    }
}
